###  http://purl.org/ceu/ebaco#winemaking_object_property

ebaco:winemaking_object_property rdf:type owl:ObjectProperty ;
                                 
                                 rdfs:label "winemaking object property"@en ,
                                            "propiedad de la vinificación"@es ;
                                 
