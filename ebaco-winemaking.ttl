<http://purl.org/ceu/ebaco/winemaking> rdf:type owl:Ontology ;
                                       
                                       rdfs:label "Ontología E-BACO"@es ;
                                       
                                       rdfs:comment """The E-BACO ontology defines a precise vocabulary for representing knowledge in the enology domain. 
The E-BACO-Core module contains the basic concepts, properties and relations for describing wines. 
It has been developped by the Complutense university of Madrid and the San Pablo CEU university."""@en ;
                                       
                                       dc:creator "Alfredo Sánchez Alberca (asalber@ceu.es) <http://aprendeconalf.es/foaf.rdf>"@es ;
                                       
                                       dc:contributor "Aranzazu Gómez Garay (magom02@bio.ucm.es)"@es ,
                                                      "Elena Pérez-Urria Carril (elenapuc@bio.ucm.es)"@es ;
                                       
                                       dc:date "Febrero 2015"@es ;
                                       
                                       rdfs:comment """La ontología E-BACO define un vocabulario preciso para representación de conocimiento en el dominio de la enología.
El módulo E-BACO-Winemaking contiene los conceptos, propiedades y relaciones para describir los vinos y su proceso de elaboración y traza desde un punto de vista de expertos (bodegueros, enólogos, etc.)
Ha sido desarrollada por la Universidad Complutense de Madrid y la Universidad San Pablo CEU."""@es ;
                                       
                                       dc:rights "La ontología E-BACO está bajo una licencia de Creative Commons Reconocimiento-CompartirIgual 3.0 de España (http://creativecommons.org/licenses/by-sa/3.0/es/)."@es ;
                                       
                                       dc:contributor "Luisa Martín (lmartin@bio.ucm.es)"@es ;
                                       
                                       dc:creator "Mari Ángeles Gómez Flechoso (magflechoso@bio.ucm.es)"@es ;
                                       
                                       dc:contributor "María Dolores Saco Sierra (mdsaco@farm.ucm.es)"@es ;
                                       
                                       owl:versionInfo "Vesión 1.0"@es ;
                                       
                                       rdfs:seeAlso <http://ebaco.github.io/> ;
                                       
                                       owl:imports <http://purl.org/ceu/ebaco/core/1.0> ;
                                       
                                       owl:versionIRI <http://purl.org/ceu/ebaco/winemaking/1.0> ;
                                       
                                       owl:imports <http://www.w3.org/ns/prov-o-20130430> .


#################################################################
#
#    Annotation properties
#
#################################################################


###  http://purl.org/ceu/ebaco#appellation_data_property

ebaco:appellation_data_property rdf:type owl:AnnotationProperty ;
                                
                                rdfs:domain <http://purl.org/ceu/ebaco/core#Appellations> .



###  http://purl.org/ceu/ebaco#appellation_of

ebaco:appellation_of rdf:type owl:AnnotationProperty .



###  http://purl.org/ceu/ebaco#carbon_dioxide_content_of_wine_g_l

ebaco:carbon_dioxide_content_of_wine_g_l rdf:type owl:AnnotationProperty ;
                                         
                                         rdfs:domain core:Sparkling_wine .



###  http://purl.org/ceu/ebaco#wine_appellation_data_property

ebaco:wine_appellation_data_property rdf:type owl:AnnotationProperty ;
                                     
                                     rdfs:domain <http://purl.org/ceu/ebaco/core#Wine_appellation> .



###  http://purl.org/ceu/ebaco#wine_region_with_appellation_object_property

ebaco:wine_region_with_appellation_object_property rdf:type owl:AnnotationProperty ;
                                                   
                                                   rdfs:domain <http://purl.org/ceu/ebaco/core#Wine_region_with_appellation> .



###  http://purl.org/ceu/ebaco#winery_data_property

ebaco:winery_data_property rdf:type owl:AnnotationProperty ;
                           
                           rdfs:domain core:Winery .





#################################################################
#
#    Object Properties
#
#################################################################


###  http://purl.org/ceu/ebaco#allows_grape_variety

ebaco:allows_grape_variety rdf:type owl:ObjectProperty ;
                           
                           rdfs:label "allows grape variety"@en ,
                                      "permite la variedad de uva"@es ;
                           
                           rdfs:comment "Propiedad que relaciona una denominación de origen con una variedad de uva permitida en la denominación."@es ;
                           
                           rdfs:range ebaco:Grape ;
                           
                           rdfs:subPropertyOf ebaco:wine_appellation_object_property .



###  http://purl.org/ceu/ebaco#appellation_of

ebaco:appellation_of rdf:type owl:ObjectProperty ;
                     
                     rdfs:subPropertyOf ebaco:wine_region_with_appellation_object_property .



###  http://purl.org/ceu/ebaco#belongs_to_wine_appellation

ebaco:belongs_to_wine_appellation rdf:type owl:FunctionalProperty ,
                                           owl:ObjectProperty ;
                                  
                                  rdfs:label "belongs to wine appellation"@en ,
                                             "pertenece a la denominación de origen"@es ;
                                  
                                  rdfs:comment "Propiedad que relaciona un viñedo con la denominación de origen a la que pertenece."@es ;
                                  
                                  rdfs:range ebaco:Wine_appellation ;
                                  
                                  rdfs:subPropertyOf ebaco:vineyard_object_property .



###  http://purl.org/ceu/ebaco#belongs_to_wine_region

ebaco:belongs_to_wine_region rdf:type owl:FunctionalProperty ,
                                      owl:ObjectProperty ;
                             
                             rdfs:label "belongs to wine region"@en ,
                                        "pertenece a la región vinícola"@es ;
                             
                             rdfs:range ebaco:Wine_region ;
                             
                             rdfs:subPropertyOf ebaco:vineyard_object_property .



###  http://purl.org/ceu/ebaco#belongs_to_winery

ebaco:belongs_to_winery rdf:type owl:FunctionalProperty ,
                                 owl:ObjectProperty ;
                        
                        rdfs:label "belongs to winery"@en ,
                                   "pertenece a bodega"@es ;
                        
                        rdfs:range ebaco:Winery ;
                        
                        rdfs:subPropertyOf ebaco:vineyard_object_property .



###  http://purl.org/ceu/ebaco#contains_vineyard

ebaco:contains_vineyard rdf:type owl:InverseFunctionalProperty ,
                                 owl:ObjectProperty ;
                        
                        rdfs:label "contains vineyard"@en ,
                                   "contiene viñedo"@es ;
                        
                        rdfs:comment "Propiedad que relaciona una región vinícola con un viñedo contenido en esa región."@es ;
                        
                        rdfs:range ebaco:Vineyard ;
                        
                        owl:inverseOf ebaco:belongs_to_wine_region ;
                        
                        rdfs:subPropertyOf ebaco:wine_region_object_property .



###  http://purl.org/ceu/ebaco#grape_allowed_in_wine_region

ebaco:grape_allowed_in_wine_region rdf:type owl:ObjectProperty ;
                                   
                                   rdfs:label "grape variety allowed in wine region"@en ,
                                              "variedad de uva permitida en región vinícola"@es ;
                                   
                                   rdfs:domain ebaco:Grape ;
                                   
                                   rdfs:range ebaco:Wine_region ;
                                   
                                   rdfs:subPropertyOf ebaco:grape_object_property .



###  http://purl.org/ceu/ebaco#grape_cultivation_object_property

ebaco:grape_cultivation_object_property rdf:type owl:ObjectProperty ;
                                        
                                        rdfs:label "grape cultivation object property"@en ,
                                                   "propiedad del cultivo de la uva"@es ;
                                        
                                        rdfs:domain ebaco:Grape_cultivation ;
                                        
                                        rdfs:subPropertyOf ebaco:winemaking_object_property .



###  http://purl.org/ceu/ebaco#grape_cultivation_type

ebaco:grape_cultivation_type rdf:type owl:ObjectProperty ;
                             
                             rdfs:label "grape cultivation type"@en ,
                                        "tipo de cultivo de la uva"@es ;
                             
                             rdfs:comment "Propiedad que relaciona un cultivo con el tipo de cultivo que es (convencional, ecológico, biodinámico, etc.)"@es ;
                             
                             rdfs:subPropertyOf ebaco:grape_cultivation_object_property .



###  http://purl.org/ceu/ebaco#grape_destemming_object_property

ebaco:grape_destemming_object_property rdf:type owl:ObjectProperty ;
                                       
                                       rdfs:label "grape destemming object property"@en ,
                                                  "propiedad del despalillado de la uva"@es ;
                                       
