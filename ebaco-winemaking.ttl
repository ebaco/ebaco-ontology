###  http://purl.org/ceu/ebaco#grape_of_vintage

ebaco:grape_of_vintage rdf:type owl:ObjectProperty ;
                       
                       rdfs:label "grape of vintage"@en ,
                                  "uva de la vendimia"@es ;
                       
                       rdfs:range ebaco:Grape ;
                       
                       rdfs:subPropertyOf ebaco:vintage_object_property .



###  http://purl.org/ceu/ebaco#grow_grape

ebaco:grow_grape rdf:type owl:FunctionalProperty ,
                          owl:ObjectProperty ;
                 
                 rdfs:label "grow grape variety"@en ,
                            "produce variedad de uva"@es ;
                 
                 rdfs:range ebaco:Grape ;
                 
                 rdfs:subPropertyOf ebaco:vineyard_object_property .



###  http://purl.org/ceu/ebaco#has_vineyard_pruning_system

ebaco:has_vineyard_pruning_system rdf:type owl:FunctionalProperty ,
                                           owl:ObjectProperty ;
                                  
                                  rdfs:label "has vineyard prunning system"@en ,
                                             "tiene sistema de poda de viñedo"@es ;
                                  
                                  rdfs:subPropertyOf ebaco:vineyard_object_property ;
                                  
                                  rdfs:range ebaco:vineyard_training .



###  http://purl.org/ceu/ebaco#use_fertilizer

ebaco:use_fertilizer rdf:type owl:ObjectProperty ;
                     
                     rdfs:label "use fertilizer"@en ,
                                "usa fertilizante"@es ;
                     
                     rdfs:range ebaco:Fertilizer ;
                     
                     rdfs:subPropertyOf ebaco:grape_cultivation_object_property .



###  http://purl.org/ceu/ebaco#use_fungicide

ebaco:use_fungicide rdf:type owl:ObjectProperty ;
                    
                    rdfs:label "use fungicide"@en ,
                               "usa fungicida"@es ;
                    
                    rdfs:range ebaco:Fungicide ;
                    
                    rdfs:subPropertyOf ebaco:grape_cultivation_object_property .



###  http://purl.org/ceu/ebaco#use_herbicide

ebaco:use_herbicide rdf:type owl:ObjectProperty ;
                    
                    rdfs:label "use herbicide"@en ,
                               "usa herbicida"@es ;
                    
                    rdfs:range ebaco:Herbicide ;
                    
                    rdfs:subPropertyOf ebaco:grape_cultivation_object_property .



###  http://purl.org/ceu/ebaco#use_pesticide

ebaco:use_pesticide rdf:type owl:ObjectProperty ;
                    
                    rdfs:label "use pesticide"@en ,
                               "usa pesticida"@es ;
                    
                    rdfs:range ebaco:Pesticide ;
                    
                    rdfs:subPropertyOf ebaco:grape_cultivation_object_property .



###  http://purl.org/ceu/ebaco#vineyard_object_property

ebaco:vineyard_object_property rdf:type owl:ObjectProperty ;
                               
                               rdfs:label "vineyard object property"@en ,
                                          "propiedad del viñedo"@es ;
                               
                               rdfs:domain ebaco:Vineyard .



###  http://purl.org/ceu/ebaco#vintage_object_property

ebaco:vintage_object_property rdf:type owl:ObjectProperty ;
                              
                              rdfs:label "vintage object property"@en ,
                                         "propiedad de la vendimia"@es ;
                              
                              rdfs:domain ebaco:Vintage ;
                              
                              rdfs:subPropertyOf ebaco:winemaking_object_property .



###  http://purl.org/ceu/ebaco#vintage_type

ebaco:vintage_type rdf:type owl:ObjectProperty ;
                   
                   rdfs:label "vintage type"@en ,
                              "tipo de vendimia"@es ;
                   
                   rdfs:comment "Propiedad que relaciona una vendimia con el tipo de vendimia (manual, mecánica, etc.)."@es ;
                   
                   rdfs:subPropertyOf ebaco:vintage_object_property .



###  http://purl.org/ceu/ebaco#wine_region_with_appellation_object_property

ebaco:wine_region_with_appellation_object_property rdf:type owl:ObjectProperty ;
                                                   
