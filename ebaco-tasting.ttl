###  http://purl.org/ceu/ebaco#done_by_taster

:done_by_taster rdf:type owl:FunctionalProperty ,
                         owl:ObjectProperty ;
                
                rdfs:label "done by taster"@en ,
                           "hecha por catador"@es ;
                
                rdfs:comment "Propiedad que relaciona una cata con el catador que la hizo."@es ;
                
                rdfs:range :Wine_taster ;
                
                rdfs:domain :Wine_tasting ;
                
                rdfs:subPropertyOf :wine_tasting_object_property .



